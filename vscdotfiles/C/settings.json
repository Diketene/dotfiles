 {
    "C_Cpp.intelliSenseEngine": "disabled",
    "clangd.path":"/usr/local/clang/bin/clangd",
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd",
    "clangd.arguments": [
        "--enable-config"
    ],
    "code-runner.executorMap": {
        "c": "cd $dir && clang $fileName -L/usr/local/openmpi/lib -lmpi -o bin/$fileNameWithoutExt && mpirun -np 4 ./bin/$fileNameWithoutExt",
        "cpp": "cd $dir && clang++ -std=c++23 -stdlib=libc++ $fileName -o bin/$fileNameWithoutExt && ./bin/$fileNameWithoutExt",
    },

    "C_Cpp.errorSquiggles": "enabled",
    "files.associations": {
        "format": "cpp",
        "iostream": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
