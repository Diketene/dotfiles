{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Fortran",
			"type": "shell",
			"command": "/gcc13.3.0/bin/gfortran",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/bin/${fileBasenameNoExtension}"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}